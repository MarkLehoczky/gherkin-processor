                "text": "the user accounts are registered with the following details:",
                "table": {
                    "username": [
                        "user@example.com"
                    ],
                    "password": [
                        "password123"
                    ]
                },
                "doc_string": null
            },
            {
                "type": "Given",
                "text": "the admin accounts are registered with the following details:",
                "table": {
                    "username": [
                        "admin@example.com"
                    ],
                    "password": [
                        "password135"
                    ]
                },
                "doc_string": null
            }
        ]
    },
